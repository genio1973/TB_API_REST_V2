var AuthService = (function () {
    function AuthService(http) {
        this.http = http;
        this.authUrl = 'http://test.romandvolley.ch/api/v3/public/user';
        this.loggedIn = false;
        this.loggedInAsAdmin = false;
        //observable src : contains data
        this.authLoginSource = new __WEBPACK_IMPORTED_MODULE_3_rxjs_Subject__["Subject"]();
        this.authLogoutSource = new __WEBPACK_IMPORTED_MODULE_3_rxjs_Subject__["Subject"]();
        // observable stream : made a subscription to this 
        this.loginConnected$ = this.authLoginSource.asObservable();
        this.logout$ = this.authLogoutSource.asObservable();
        // look at localStorage to check if the user is logged in
        this.loggedIn = !!localStorage.getItem('token');
    }
    /**
     * Check if the user is logged in
     */
    AuthService.prototype.isLoggedIn = function () {
        return this.loggedIn;
    };
    /**
   * Check if the user is logged in as super-administrator
   */
    AuthService.prototype.isLoggedInAsAdmin = function () {
        return this.loggedInAsAdmin;
    };
    /**
     * Log the user in
     */
    AuthService.prototype.login = function (email, password) {
        var _this = this;
        return this.http.post(this.authUrl + "/login", { 'email': email, 'password': password }, this.headBuilder)
            .map(function (res) { return res.json().result; })
            .do(function (res) {
            if (!res.error) {
                localStorage.setItem('token', res.token);
                localStorage.setItem('id_user', res.id_user);
                localStorage.setItem('email', res.email);
                localStorage.setItem('droits', res.droits);
                _this.authLoginSource.next(email + " (" + res.droits + ")");
                _this.loggedIn = true;
                if (res.id_role == 1) {
                    _this.loggedInAsAdmin = true;
                }
            }
        })
            .catch(this.handleError);
    };
    /**
     * Header preparation, contentType
     **/
    AuthService.prototype.headBuilder = function () {
        var headers = new __WEBPACK_IMPORTED_MODULE_1__angular_http__["b" /* Headers */]();
        headers.append('Content-Type', 'application/json');
        return new __WEBPACK_IMPORTED_MODULE_1__angular_http__["c" /* RequestOptions */]({ headers: headers }); // Create a request option
    };
    /**
     * Log the user out
     */
    AuthService.prototype.logout = function () {
        localStorage.removeItem('token');
        localStorage.removeItem('id_user');
        localStorage.removeItem('email');
        localStorage.removeItem('droits');
        this.authLogoutSource.next();
        this.loggedIn = false;
        this.loggedInAsAdmin = false;
    };
    /**
     * Handle any errors from the API
     */
    AuthService.prototype.handleError = function (err) {
        var errMessage;
        if (err instanceof __WEBPACK_IMPORTED_MODULE_1__angular_http__["d" /* Response */]) {
            var body = err.json() || '';
            var error = body.error || JSON.stringify(body);
            errMessage = err.status + " - " + (err.statusText || '') + " " + error;
        }
        else {
            errMessage = err.message ? err.message : err.toString();
        }
        return __WEBPACK_IMPORTED_MODULE_2_rxjs_Observable__["Observable"].throw(errMessage);
    };
